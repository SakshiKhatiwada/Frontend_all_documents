<!-- OR directly giving namespace to root -->
<root xmlns:h="http://www.w3.org"
    xmlns:f="http://www.w3.org/furniture">
    <h:table>
        <h:tr>
            <h:td>Apple</h:td>
            <h:td>Banana</h:td>
        </h:tr>
    </h:table>

    <f:table>
        <f:tr>
            <f:td>Apple</f:td>
            <f:td>Banana</f:td>
        </f:tr>
    </f:table>
</root>

